{
  "name": "gatsby-i18n",
  "private": true,
  "description": "Gatsby example site using MDX and dependency-free i18n",
  "author": "Vladimir Zhelvis",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "prettier": "prettier --write ./\\*\\*/*.{js,css,mdx,json}",
    "deploy": "npm run build && gh-pages -d public -b master"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "@reach/router": "^1.2.1",
    "@theme-ui/prism": "^0.2.46",
    "gatsby": "^2.17.10",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-google-analytics": "^2.1.29",
    "gatsby-plugin-manifest": "^2.2.29",
    "gatsby-plugin-mdx": "^1.0.55",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.3.2",
    "gatsby-plugin-sitemap": "^2.2.22",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "gatsby-remark-images": "^3.1.33",
    "gatsby-source-filesystem": "^2.1.35",
    "gatsby-transformer-json": "^2.2.16",
    "gatsby-transformer-sharp": "^2.3.5",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-switch": "^5.0.1",
    "theme-ui": "^0.2.46"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
