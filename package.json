{
  "name": "gatsby-i18n",
  "private": true,
  "description": "Gatsby example site using MDX and dependency-free i18n",
  "author": "Vladimir Zhelvis",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby clean && gatsby build",
    "serve": "gatsby serve",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "prettier": "prettier --write ./\\*\\*/*.{js,css,mdx,json}",
    "deploy": "npm run build && gh-pages -d public -b master"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@mdx-js/mdx": "^1.6.16",
    "@reach/router": "^1.3.4",
    "@theme-ui/prism": "^0.4.0-rc.1",
    "gatsby": "^2.24.2",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-google-analytics": "^2.3.11",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-mdx": "^1.2.25",
    "gatsby-plugin-offline": "^3.2.17",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.6.19",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-theme-ui": "^0.4.0-rc.1",
    "gatsby-remark-images": "^3.3.18",
    "gatsby-source-filesystem": "^2.3.19",
    "gatsby-transformer-json": "^2.4.11",
    "gatsby-transformer-sharp": "^2.5.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "theme-ui": "^0.4.0-rc.1"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
